<div class="main-card mb-3 card">
    <div class="card-body col" id="messages" name="messages" style="max-height: 350px;min-height:300px;overflow-y: scroll;">
        <div class="table-responsive">
            <table class="align-middle mb-0 table table-borderless table-striped table-hover" id="message_users_tb">
                <thead>
                <tr>
                    <th class="text-left">From</th>
                    <th class="text-left">Message</th>
                    <th class="text-left">Date Received</th>
                </tr>
                </thead>
                <tbody>
                <%inbox.map(i =>{ %>
                <tr>
                    <td class="text-left"><%=i.Sender.firstname%> <%=i.Sender.lastname%></td>
                    <td>
                        <a href="/user/message-room/<%=i.SenderId%>">
                            <div class="widget-content p-0">
                            <div class="widget-content-wrapper">
                                <div class="widget-content-left flex2">
                                    <div class="widget-heading" ><%=i.message%></div>
                                </div>
                            </div>
                        </div>
                        </a>
                    </td>
                    <td class="text-left">
                        <%=i.createdAt%>
                    </td>
                </tr>
                <%  }); %>
                </tbody>
            </table>
        </div>
    </div>
    <div class="card-footer row">

    </div>
</div>